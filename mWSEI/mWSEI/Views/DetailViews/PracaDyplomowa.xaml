<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="mWSEI.Views.DetailViews.PracaDyplomowa"
             Title="Praca dyplomowa">
    <ContentPage Title="Praca dyplomowa">
        <ContentPage.Content>
            <StackLayout VerticalOptions="FillAndExpand" IsVisible="False">
                <StackLayout x:Name="LogoStack">
                    <Label x:Name="AlertNoInternet" HorizontalTextAlignment="Center" TextColor="White" BackgroundColor="Red" />
                </StackLayout>

                <Label Text="Praca dyplomowa"
                    VerticalOptions="CenterAndExpand" 
                    HorizontalOptions="CenterAndExpand" />
            </StackLayout>
        </ContentPage.Content>
    </ContentPage>

    <ContentPage Title="Zapisy na prace dyplomowe">
        <ContentPage.Content>
            <StackLayout VerticalOptions="FillAndExpand">

                <Label Text="Zapisy na prace dyplomowe"
                    VerticalOptions="CenterAndExpand" 
                    HorizontalOptions="CenterAndExpand" />
            </StackLayout>
        </ContentPage.Content>
    </ContentPage>
    
</TabbedPage>